{
    "name": "wizacha/bbq",
    "description": "Message Queue abstraction library for PHP, based on eventio/bbq",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Wizacha Dev Team",
            "email": "dev@wizacha.com",
            "homepage": "http://wizacha.com"
        }
    ],
    "support": {
        "source": "https://bitbucket.org/wizacha/bbq/src"
    },
    "require": {
        "eventio/bbq": "~1.0",
        "aws/aws-sdk-php": "2.*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/wizacha/bbq.git"
        }
    ],
    "require-dev": {
        "atoum/atoum": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Wizacha\\": "src/"
        }
    }
}
